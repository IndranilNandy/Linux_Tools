# Aliases for GENERIC commands

c=clear
cg=c;g
sctl=systemctl
lsa=ls -alut

p+x=find . -name "*.sh" -exec chmod +x {} \;
p-x=find . -name "*.sh" -exec chmod -x {} \;
p=find . -name "*.sh" -exec ls -alut {} \;

ccom=cd /usr/local/bin/mycommands
cref= cd /usr/local/bin/myscriptrefs
ctool=cd $HOME/MyTools/Linux_Tools

s=shutdown now

listjava=sdk list java | grep "installed"
listgradle=sdk list gradle | grep "\*" | grep -v "installed"

sstarter=springstarter

k700=kill $(sudo lsof -i -P -n | grep -E "7000 | 7001 | 7002 | 7003" | awk '\''{ print $2 }'\'')
k8080=kill $(sudo lsof -i -P -n | grep -E "8080" | awk '\''{ print $2 }'\'')

cleanmycommandsrepo=cd /usr/local/bin/mycommands && sudo rm
cleanmyscriptrefs=cd /usr/local/bin/myscriptrefs && sudo rm
