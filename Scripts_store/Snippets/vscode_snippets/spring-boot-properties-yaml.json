{
	// Place your snippets for spring-boot-properties-yaml here. Each snippet is defined under a snippet name and has a prefix, body and
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	// *****************************************************************************
    // USE CTRL + SHIFT - AND THEN CTRL +
	// *****************************************************************************
	// Common configurations
	// *****************************************************************************
    "Common Config": {
        "prefix": [
			"[snp] Common Config",
			"[sb-config] Common Config",
		],
        "body": [
            "spring:",
            "\tapplication:",
            "\t\tname: productsvc",
            "\tconfig:",
            "\t\timport:",
            "\t\t\t- file:./config/application/application-root.yaml",
            "\t\t\t- optional:file:./config/application/external.yaml",
            "\t\t\t# - optional:file:.env[.properties]",
            "\tprofiles:",
            "\t\tinclude:",
            "\t\t\t- actuator",
            "\t\t\t- logging",
            "\t\tgroup:",
            "\t\t\tdevpsql:",
            "\t\t\t\t- postgresql",
            "\t\t\t\t- jpa",
            "\t\t\t\t- hibernate",
            "\t\t\t\t- initdb",
            "\t\t\tdevmysql:",
            "\t\t\t\t- mysql",
            "\t\t\t\t- jpa",
            "\t\t\t\t- hibernate",
            "\t\t\ttest:",
            "\t\t\t\t- h2",
            "\t\t\t\t- jpa",
            "\t\t\t\t- hibernate",
            "\t\tactive:",
            "\t\t\t- test",
            "",
            "server:",
            "\tport: 8081",
            "",
            "# debug: true",
            "# trace: true",
        ],
        "description": "Application yaml - common config"
    },
    "Root Config": {
        "prefix": [
			"[snp] Root Config",
			"[sb-config] Root Config",
		],
        "body": [
            "custom:",
            "\tspring:",
            "\t\tprofiles:",
            "",
            "\t\t\t# PROFILES-TYPE is JUST for the sake of INFORMATION. This IS NOT A GROUP. Instead, a particualy type just documents all the available options.",
            "\t\t\t# All of them may NOT be enabled simultaneously. No GROUP or anything can refer to a TYPE. This serves as a DOCUMENTATION only.",
            "\t\t\ttype:",
            "\t\t\t\tdb-schema-gen:",
            "\t\t\t\t\t# If db-schema-gen-jpa is ENABLED, this will have NO EFFECT",
            "\t\t\t\t\t- db-schema-gen-hb",
            "\t\t\t\t\t# This will override db-schema-gen-hb",
            "\t\t\t\t\t- db-schema-gen-jpa",
            "\t\t\t\t\t# Do NOT use this along with Database Migration Tools",
            "\t\t\t\t\t- db-schema-gen-sb",
            "\t\t\t\t\t- liquibase",
            "\t\t\t\tdb-init-script:",
            "\t\t\t\t\t- db-init-script-hb",
            "\t\t\t\t\t# Do NOT use this along with Database Migration Tools",
            "\t\t\t\t\t- db-init-script-sb",
            "",
            "\t\t\t# These custom PROFILES-GROUP are referred from spring.profiles.group. Any custom GROUP CANNOT refer to a custom PROFILES-TYPE.",
            "\t\t\t# It can ONLY refer to a custom GROUP or a spring.profiles.group or any spring profile",
            "\t\t\tgroup: &profiles-group",
            "\t\t\t\tgrp-initdb-jpa:",
            "\t\t\t\t\t- db-schema-gen-jpa",
            "\t\t\t\tgrp-initdb-hb:",
            "\t\t\t\t\t- db-schema-gen-hb",
            "\t\t\t\t\t- db-init-script-hb",
            "\t\t\t\tgrp-initdb-sb:",
            "\t\t\t\t\t- db-schema-gen-sb",
            "\t\t\t\t\t- db-init-script-sb",
            "\t\t\t\tgrp-initdb-liquibase:",
            "\t\t\t\t\t- liquibase",
            "\t\t\t\tgrp-initdb-nodmt:",
            "\t\t\t\t\t- nodmt-commons",
            "\t\t\t\t\t- grp-initdb-jpa",
            "\t\t\t\t\t- grp-initdb-hb",
            "\t\t\t\t\t- grp-initdb-sb",
            "\t\t\t\tgrp-initdb-dmt:",
            "\t\t\t\t\t- grp-initdb-liquibase",
            "\t\t\t\tgrp-initdb:",
            "\t\t\t\t\t# - grp-initdb-nodmt",
            "\t\t\t\t\t- grp-initdb-dmt",
            "",
            "",
            "spring:",
            "\tprofiles:",
            "\t\tgroup: *profiles-group"
        ],
        "description": "Application yaml - Root config"
    },

	// *****************************************************************************
	// Database configurations
	// *****************************************************************************
    "H2 Config": {
        "prefix": [
			"[snp] H2 Config",
			"[sb-config] H2 Config",
		],
        "body": [
            "---",
            "",
            "spring:",
            "\tconfig:",
            "\t\tactivate:",
            "\t\t\ton-profile:",
            "\t\t\t\t- h2",
            "\tdatasource:",
            "\t\turl: jdbc:h2:mem:\\${H2_DATABASE};DB_CLOSE_ON_EXIT=FALSE",
            "\t\tdriver-class-name: org.h2.Driver",
            "\t\tusername: \\${spring.application.name}",
            "\t\tpassword: \\${H2_DATABASE_PASSWORD}",
            "\th2:",
            "\t\tconsole:",
            "\t\t\tenabled: true"
        ],
        "description": "H2 Config"
    },
    "MySql Config": {
        "prefix": [
			"[snp] MySql Config",
			"[sb-config] MySql Config",
		],
        "body": [
            "---",
            "",
            "spring:",
            "\tconfig:",
            "\t\tactivate:",
            "\t\t\ton-profile:",
            "\t\t\t\t- mysql",
            "\tdatasource:",
            "\t\turl: jdbc:mysql://\\${MYSQL_DATABASE_HOST:localhost}:\\${MYSQL_DATABASE_PORT:3306}/\\${MYSQL_DATABASE}",
            "\t\tdriver-class-name: com.mysql.cj.jdbc.Driver",
            "\t\tusername: \\${spring.application.name}",
            "\t\tpassword: \\${MYSQL_DATABASE_PASSWORD}",
        ],
        "description": "MySql Config"
    },
    "PostgreSql Config": {
        "prefix": [
			"[snp] PostgreSql Config",
			"[sb-config] PostgreSql Config",
		],
        "body": [
            "---",
            "",
            "spring:",
            "\tconfig:",
            "\t\tactivate:",
            "\t\t\ton-profile:",
            "\t\t\t\t- postgresql",
            "\tdatasource:",
            "\t\turl: jdbc:postgresql://\\${POSTGRESQL_DATABASE_HOST:localhost}:\\${POSTGRESQL_DATABASE_PORT:5432}/\\${POSTGRESQL_DATABASE}",
            "\t\tdriver-class-name: org.postgresql.Driver",
            "\t\tusername: \\${spring.application.name}",
            "\t\tpassword: \\${POSTGRESQL_DATABASE_PASSWORD}",
        ],
        "description": "PostgreSql Config"
    },
    "Jpa Config": {
        "prefix": [
			"[snp] Jpa Config",
			"[sb-config] Jpa Config",
		],
        "body": [
            "---",
            "",
            "spring:",
            "\tconfig:",
            "\t\tactivate:",
            "\t\t\ton-profile:",
            "\t\t\t\t- jpa",
            "\tjpa:",
            "\t\tshow-sql: true",
            "\tliquibase:",
            "\t\tenabled: false",
        ],
        "description": "Jpa Config"
    },
    "Hibernate Config": {
        "prefix": [
			"[snp] Hibernate Config",
			"[sb-config] Hibernate Config",
		],
        "body": [
            "---",
            "",
            "spring:",
            "\tconfig:",
            "\t\tactivate:",
            "\t\t\ton-profile:",
            "\t\t\t\t- hibernate"
        ],
        "description": "Hibernate Config"
    },
    "Db-init-script-hb Config": {
        "prefix": [
			"[snp] Db-init-script-hb Config",
			"[sb-config] Db-init-script-hb Config",
		],
        "body": [
            "---",
            "",
            "spring:",
            "\tconfig:",
            "\t\tactivate:",
            "\t\t\ton-profile:",
            "\t\t\t\t- db-init-script-hb",
            "\tjpa:",
            "\t\tproperties:",
            "\t\t\t\"[hibernate.hbm2ddl.import_files]\": \\${custom.hibernate.init.import_files}",
            "\t\t\t# \"[hibernate.hbm2dll.create_namespaces]\": true",
            "\t\t\t# same as hibernate.ddl-auto",
            "\t\t\t# \"[hibernate.hbm2ddl.auto]\": create-drop",
            "",
            "custom:",
            "\thibernate:",
            "\t\tinit:",
            "\t\t\timport:",
            "\t\t\t\tlocation: \"/hb-db-init\"",
            "\t\t\t\tfiles:",
            "\t\t\t\t\t- import-script-01.sql",
            "\t\t\t\t\t- import-script-02.sql",
            "\t\t\timport_files: >",
            "\t\t\t\t\\${custom.hibernate.init.import.location}/\\${custom.hibernate.init.import.files.0},",
            "\t\t\t\t\\${custom.hibernate.init.import.location}/\\${custom.hibernate.init.import.files.1},"
        ],
        "description": "Db-init-script-hb Config"
    },
    "Db-init-script-sb Config": {
        "prefix": [
			"[snp] Db-init-script-sb Config",
			"[sb-config] Db-init-script-sb Config",
		],
        "body": [
            "---",
            "custom:",
            "\tsql:",
            "\t\tinit:",
            "\t\t\tlocation:",
            "\t\t\t\tdata: classpath:/spring-db-init",
            "\t\t\tdata-files: &data-files",
            "\t\t\t\t- \\${custom.sql.init.location.data}/data-postgresql-01.sql",
            "\t\t\t\t- \\${custom.sql.init.location.data}/data-postgresql-02.sql",
            "",
            "spring:",
            "\tconfig:",
            "\t\tactivate:",
            "\t\t\ton-profile:",
            "\t\t\t\t- db-init-script-sb",
            "\tsql:",
            "\t\tinit:",
            "\t\t\t# Platform to use in the default schema or data script locations, schema-\\${platform}.sql and data-\\${platform}.sql. Not needed here.",
            "\t\t\t# platform: postgresql",
            "\t\t\t# To enable 'always initialize' for script-based DataSource initialization using 'schema.sql' and 'data.sql'. Default: never",
            "\t\t\tmode: always",
            "\t\t\tdata-locations: *data-files",
            "\tjpa:",
            "\t\t# If you want script-based DataSource initialization to be able to build upon the schema creation performed by Hibernate, e.g. 'data.sql' to run after schema/relation creation",
            "\t\tdefer-datasource-initialization: true"
        ],
        "description": "Db-init-script-sb Config"
    },
    "Db-schema-gen-hb Config": {
        "prefix": [
			"[snp] Db-schema-gen-hb Config",
			"[sb-config] Db-schema-gen-hb Config",
		],
        "body": [
            "---",
            "",
            "spring:",
            "\tconfig:",
            "\t\tactivate:",
            "\t\t\ton-profile:",
            "\t\t\t\t- db-schema-gen-hb",
            "\tjpa:",
            "\t\thibernate:",
            "\t\t\t# If jpa schema-generation properties are used, this property is NOT effective",
            "\t\t\tddl-auto: create-drop"
        ],
        "description": "Db-schema-gen-hb Config"
    },
    "Db-schema-gen-jpa Config": {
        "prefix": [
			"[snp] Db-schema-gen-jpa Config",
			"[sb-config] Db-schema-gen-jpa Config",
		],
        "body": [
            "---",
            "custom:",
            "\tjpa:",
            "\t\tschema-generation:",
            "\t\t\tlocation:",
            "\t\t\t\tcreate: ./generated/jpa/schema/create.sql",
            "\t\t\t\tdrop: ./generated/jpa/schema/drop.sql",
            "",
            "spring:",
            "\tconfig:",
            "\t\tactivate:",
            "\t\t\ton-profile:",
            "\t\t\t\t- db-schema-gen-jpa",
            "\tjpa:",
            "\t\tproperties:",
            "\t\t\t# ---- SCHEMA GENERATION properties ----",
            "\t\t\t# This overrides jpa.hibernate.ddl-auto property",
            "\t\t\tjakarta:",
            "\t\t\t\tpersistence:",
            "\t\t\t\t\tschema-generation:",
            "\t\t\t\t\t\tscripts:",
            "\t\t\t\t\t\t\taction: drop-and-create",
            "\t\t\t\t\t\t\tcreate-target: \\${custom.jpa.schema-generation.location.create}",
            "\t\t\t\t\t\t\tcreate-source: metadata",
            "\t\t\t\t\t\t\tdrop-target: \\${custom.jpa.schema-generation.location.drop}",
            "\t\t\t\t\t\t\tdrop-source: metadata",
            "\t\t\t\t\t\tdatabase:",
            "\t\t\t\t\t\t\taction: create-drop",
            "\t\tshow-sql: true"
        ],
        "description": "Db-schema-gen-jpa Config"
    },
    "Db-schema-gen-sb Config": {
        "prefix": [
			"[snp] Db-schema-gen-sb Config",
			"[sb-config] Db-schema-gen-sb Config",
		],
        "body": [
            "---",
            "custom:",
            "\tsql:",
            "\t\tinit:",
            "\t\t\tlocation:",
            "\t\t\t\tschema: classpath:/spring-db-init",
            "\t\t\tschema-files: &schema-files",
            "\t\t\t\t- \\${custom.sql.init.location.schema}/schema-postgresql-01.sql",
            "",
            "spring:",
            "\tconfig:",
            "\t\tactivate:",
            "\t\t\ton-profile:",
            "\t\t\t\t- db-schema-gen-sb",
            "\tsql:",
            "\t\tinit:",
            "\t\t\t# Platform to use in the default schema or data script locations, schema-\\${platform}.sql and data-\\${platform}.sql. Not needed here.",
            "\t\t\t# platform: postgresql",
            "\t\t\t# To enable 'always initialize' for script-based DataSource initialization using 'schema.sql' and 'data.sql'. Default: never",
            "\t\t\tmode: always",
            "\t\t\tschema-locations: *schema-files",
            "\tjpa:",
            "\t\t# If you want script-based DataSource initialization to be able to build upon the schema creation performed by Hibernate, e.g. 'data.sql' to run after schema/relation creation",
            "\t\tdefer-datasource-initialization: true"
        ],
        "description": "Db-schema-gen-sb Config"
    },
    "Nodmt-commons Config": {
        "prefix": [
			"[snp] Nodmt-commons Config",
			"[sb-config] Nodmt-commons Config",
		],
        "body": [
            "---",
            "",
            "spring:",
            "\tconfig:",
            "\t\tactivate:",
            "\t\t\ton-profile:",
            "\t\t\t\t- nodmt-commons",
            "",
            "\tliquibase:",
            "\t\tenabled: false"
        ],
        "description": "Nodmt-commons Config"
    },
    "Liquibase Config": {
        "prefix": [
			"[snp] Liquibase Config",
			"[sb-config] Liquibase Config",
		],
        "body": [
            "---",
            "",
            "spring:",
            "\tconfig:",
            "\t\tactivate:",
            "\t\t\ton-profile:",
            "\t\t\t\t- liquibase",
            "\tliquibase:",
            "\t\tchange-log: classpath:db/changelog/db-changelog-root.sql",
            "\t\turl: jdbc:postgresql://\\${POSTGRESQL_DATABASE_HOST:localhost}:\\${POSTGRESQL_DATABASE_PORT:5432}/\\${POSTGRESQL_DATABASE}",
            "\t\tuser: \\${spring.application.name}",
            "\t\tpassword: \\${POSTGRESQL_DATABASE_PASSWORD}",
            "\t\t# driver-class-name: org.postgresql.Driver"
        ],
        "description": "Liquibase Config"
    },

	// *****************************************************************************
	// Logging configurations
	// *****************************************************************************
    "Logging Config": {
        "prefix": [
			"[snp] Logging Config",
			"[sb-config] Logging Config",
		],
        "body": [
            "# Ref.: https://docs.spring.io/spring-boot/docs/current/reference/html/features.html#features.logging",
            "spring:",
            "\tconfig:",
            "\t\tactivate:",
            "\t\t\ton-profile:",
            "\t\t\t- logging",
            "",
            "custom:",
            "\tlogging:",
            "\t\tpattern:",
            "\t\t\tverbosity:",
            "\t\t\t\tlevel1: \"%d{ISO8601_OFFSET_DATE_TIME_HHCMM} %highlight{%-5p} %pid --- [%t][%T] %style{[%-40.40logger{1.}]}{magenta}: %m%highlight{%rEx{full}}%n\"",
            "\t\t\t\tlevel2: \"%d{ISO8601_OFFSET_DATE_TIME_HHCMM} %highlight{%-5p} %pid --- [%t][%T] %style{[%-40.40logger{1.}]}{magenta} %style{[%-40.40class{1.}]}{blue}: %m%highlight{%rEx{full}}%n\"",
            "\t\t\t\tlevel3: \"%d{ISO8601_OFFSET_DATE_TIME_HHCMM} %highlight{%-5p} %pid --- [%t][%T] %style{[%-50.50logger{1.}]}{magenta} %style{[%-90.90location{1.}]}{cyan}: %m%highlight{%rEx{full}}%n\"",
            "\t\t\t\tlevel4: \"%d{ISO8601_OFFSET_DATE_TIME_HHCMM} %highlight{%-5p} %pid --- [%t][%T] %style{[%-50.50logger{1.}]}{magenta} %style{[%-50.50class{1.}]}{blue} %style{[%-90.90location{1.}]}{cyan}: %m%highlight{%rEx{full}}%n\"",
            "\t\t\t\tlevel5: \"%d{ISO8601_OFFSET_DATE_TIME_HHCMM} [%N] %highlight{%-5p} %pid --- [%t][%T] %style{[%-50.50logger{1.}]}{magenta} %style{[%-50.50class{1.}]}{blue} %style{[%-90.90location{1.}]}{cyan}: %m%highlight{%rEx{full}}%n\"",
            "\t\t\t\tlevel6: \"%d{ISO8601_OFFSET_DATE_TIME_HHCMM} [%N] %highlight{%-5p} %pid --- [%t][%T] %style{[%-50.50logger{1.}]}{magenta} %style{[%-50.50class{1.}]}{blue} %style{[%-90.90location{1.}]}{cyan} << %map %style{%x}{BG_Blue} %style{%X}{BG_Yellow} %style{%notEmpty{[%marker]}}{Yellow} >>: %m%highlight{%rEx{full}}%n\"",
            "",
            "logging:",
            "\t# This will override logging.file.name settings. This is needed ONLY IF you want to provide your own log4j2.yaml",
            "\t# TURNING OFF to use default logger configuration as of now, mainly due to Console output pattern still not finalized",
            "\tconfig: ./config/log-config/log4j2-spring-root.yaml",
            "\tlog4j2:",
            "\t\tconfig:",
            "\t\t\toverride:",
            "\t\t\t\t- ./config/log-config/log4j2-spring-app.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-controller.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-service.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-repository.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-model.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-autoconfiguration.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-beans.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-boot.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-jpa.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-jdbc.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-orm.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-webmvc.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-postgresql.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-mysql.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-jackson.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-jakarta.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-hibernate.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-boot-test.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-autoconfiguration-test.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-web-group.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-sql-group.yaml",
            "\t\t\t\t- ./config/log-config/log4j2-spring-testframework-group.yaml",
            "\t# file:",
            "\t\t# This is effective only if log4j2.yaml isn't explicitly created or logging.config isn't declared",
            "\t\t# name: ./generated/logs/spring-logs/\\${spring.application.name}.log",
            "\t\t# This is not working along with logging.file.path. Hench commenting out.",
            "\t\t# path: ./applog",
            "\tpattern:",
            "\t\t# This will NOT override your confiuration set in log4j2.yaml",
            "\t\tconsole: \\${custom.logging.pattern.verbosity.level1}",
            "\t\tfile: \\${custom.logging.pattern.verbosity.level1}",
            "\tthreshold:",
            "\t\t# set at default \"trace\"",
            "\t\t# This will NOT override your confiuration set in log4j2.yaml",
            "\t\tconsole: \"trace\"",
            "\t\tfile: \"trace\"",
            "\tgroup:",
            "\t\ttomcat: \"org.apache.catalina,org.apache.coyote,org.apache.tomcat\"",
            "\t# level:",
            "\t\t# set at default \"info\"",
            "\t\t# CAUTION: This will override your confiuration set in log4j2.yaml",
            "\t\t# web: \"info\"",
            "\t\t# sql: \"info\"",
            "\t\t# root: \"info\"",
            "\t\t# tomcat: \"info\"",
            "\t\t# \"[org.hibernate]\": \"info\"",
            "\t\t# \"[org.springframework.web]\": \"trace\"",
            "\t\t# \"[org.springframework.boot]\": \"info\"",
            "\t\t# \"[com.mydomain.core.product]\": \"debug\""
        ],
        "description": "Application yaml - Logging config"
    },

	// *****************************************************************************
	// Dev configurations
	// *****************************************************************************
    "Actuator Config": {
        "prefix": [
			"[snp] Actuator Config",
			"[sb-config] Actuator Config",
		],
        "body": [
            "---",
            "",
            "spring:",
            "\tconfig:",
            "\t\tactivate:",
            "\t\t\ton-profile:",
            "\t\t\t\t- actuator",
            "",
            "management:",
            "\tendpoints:",
            "\t\tweb:",
            "\t\t\texposure:",
            "\t\t\t\tinclude:",
            "\t\t\t\t- \"*\"",
        ],
        "description": "Actuator Config"
    },
}