    "DefaultRolloverStrategy": {
        "prefix": [
			"[snp] DefaultRolloverStrategy",
			"[log4j2] DefaultRolloverStrategy",
		],
        "body": [
            "# The default rollover strategy accepts both a date/time pattern and an integer from the filePattern attribute specified on the RollingFileAppender itself. If the date/time pattern is present it will be replaced with the current date and time values. If the pattern contains an integer it will be incremented on each rollover. If the pattern contains both a date/time and integer in the pattern the integer will be incremented until the result of the date/time pattern changes.",
            "DefaultRolloverStrategy:",
            "\tmax: ${1:20}\t\t\t\t# [Default: 7] The maximum value of the counter. Once this values is reached older archives will be deleted on subsequent rollovers.",
            "\tmin: ${2:1}\t\t\t\t# [Default: 1] The minimum value of the counter.",
            "\tfileIndex: ${3|max,min,nomax|}\t\t# [Default: max] [max/min/nomax] If set to \"max\" (the default), files with a higher index will be newer than files with a smaller index. If set to \"min\", file renaming and the counter will follow the Fixed Window strategy described above. Finally, as of release 2.8, if the fileIndex attribute is set to \"nomax\" then the min and max values will be ignored and file numbering will increment by 1 and each rollover will have an incrementally higher value with no maximum number of files.",
            "\t# compressionLevel: ${4:9}\t # Sets the compression level, 0-9, where 0 = none, 1 = best speed, through 9 = best compression. Only implemented for ZIP files.",
            "\t# tempCompressedFilePattern: ${5:archived-app-%d{yyyy-MM-dd-HH-mm\\}-%i.log}\t   # The pattern of the file name of the archived log file during compression.",
            "\tDelete:",
            "\t\tbasePath: ${${6:archive-rolling-path}}",
            "\t\tmaxDepth: ${7:2}",
            "\t\t# testMode: ${8|false,true|}\t  # [Default: false] If true, files are not deleted but instead a message is printed to the status logger at INFO level. Use this to do a dry run to test if the configuration works as expected.",
            "\t\tIfFileName:",
            "\t\t\tglob: ${9:\"*/app-*.log.gz\"}",
            "\t\tIfLastModified:",
            "\t\t\tage: ${10:PT10S}\t# https://logging.apache.org/log4j/2.x/log4j-core/apidocs/org/apache/logging/log4j/core/appender/rolling/action/Duration.html#parseCharSequence"
        ],
        "description": "DefaultRolloverStrategy"
    },
    "DirectWriteRolloverStrategy": {
        "prefix": [
			"[snp] DirectWriteRolloverStrategy",
			"[log4j2] DirectWriteRolloverStrategy",
		],
        "body": [
            "# The DirectWriteRolloverStrategy causes log events to be written directly to files represented by the file pattern. With this strategy file renames are not performed. If the size-based triggering policy causes multiple files to be written during the specified time period they will be numbered starting at one and continually incremented until a time-based rollover occurs.",
            "DirectWriteRolloverStrategy:",
            "\tmaxFiles: ${1:10}\t\t\t#[Default: omitted] The maximum number of files to allow in the time period matching the file pattern. If the number of files is exceeded the oldest file will be deleted. If specified, the value must be greater than 1. If the value is less than zero or omitted then the number of files will not be limited.",
            "\t# compressionLevel: ${2:9}\t# Sets the compression level, 0-9, where 0 = none, 1 = best speed, through 9 = best compression. Only implemented for ZIP files.",
            "\t# tempCompressedFilePattern: ${3:archived-app-%d{yyyy-MM-dd-HH-mm\\}-%i.log}\t\t# The pattern of the file name of the archived log file during compression."
        ],
        "description": "DirectWriteRolloverStrategy"
    },
    "PatternLayout": {
        "prefix": [
			"[snp] PatternLayout",
			"[log4j2] PatternLayout",
		],
        "body": [
            "PatternLayout:",
            "\theader: \"%style{Start of logging. [%d]}{BG_Green}%n\"",
            "\tfooter: \"%style{End of logging. [%d]}{BG_Green}%n\"",
            "\tpattern: \"%d %highlight{%-5p} %logger{1.} %class{1.} %location %pid %map [%t][%T] %style{%x}{BG_Blue} %style{%X}{BG_Yellow} %style{%notEmpty{[%marker]}}{Yellow}: %m%n\""
        ],
        "description": "PatternLayout"
    },
    "PatternLayout - Simple": {
        "prefix": [
			"[snp] PatternLayout - Simple",
			"[log4j2] PatternLayout - Simple",
		],
        "body": [
            "PatternLayout:",
            "\theader: \"%style{Start of logging. [%d]}{BG_Green}%n\"",
            "\tfooter: \"%style{End of logging. [%d]}{BG_Green}%n\"",
            "\tpattern: \"%d{ISO8601_OFFSET_DATE_TIME_HHCMM} %highlight{%-5p} %pid --- [%t][%T] %style{[%-40.40logger{1.}]}{magenta}: %m%n%highlight{%rEx{full}}%n\""
        ],
        "description": "PatternLayout - Simple"
    }
