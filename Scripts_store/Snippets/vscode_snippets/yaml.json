        "\t\t\t\tPatternLayout:",
        "\t\t\t\t\theader: \"%d{ISO8601_OFFSET_DATE_TIME_HHCMM} Start of logging.%n%n\"",
        "\t\t\t\t\tfooter: \"%n%d{ISO8601_OFFSET_DATE_TIME_HHCMM} End of logging.%n%n%n\"",
        "\t\t\t\t\tpattern: \\${currentLogLevel}",
        "\t\t\t\tPolicies:",
