# Change to dark theme
# https://fostips.com/light-dark-command-ubuntu-22-04/
org.gnome.desktop.interface color-scheme 'prefer-dark'

# https://linuxconfig.org/how-to-customize-dock-panel-on-ubuntu-20-04-focal-fossa-linux
org.gnome.shell.extensions.dash-to-dock dock-position LEFT
org.gnome.shell.extensions.dash-to-dock dash-max-icon-size 24
org.gnome.shell.extensions.dash-to-dock extend-height false
org.gnome.shell.extensions.dash-to-dock unity-backlit-items false
org.gnome.shell.extensions.dash-to-dock click-action minimize

org.gnome.desktop.screensaver ubuntu-lock-on-suspend false
org.gnome.desktop.screensaver logout-enabled false
org.gnome.desktop.screensaver lock-enabled false
org.gnome.desktop.screensaver lock-delay 0